<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jones.jonesmanlapazapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main_Activity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Single_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Versus_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Coop_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Scores_Activity"
            android:screenOrientation="portrait" />
        <activity android:name=".Jones_Activity" />
        <activity android:name=".Settings_Activity"></activity>
    </application>

</manifest>